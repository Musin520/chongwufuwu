# 环境变量配置示例文件
# 复制此文件为 .env 并填入实际值

# 数据库配置
DATABASE_URL=jdbc:mysql://localhost:3306/chongwufuwu?serverTimezone=GMT%2B8&useSSL=false
DATABASE_USERNAME=root
DATABASE_PASSWORD=your_database_password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_SSL=false

# OSS配置
OSS_ENDPOINT=https://oss-cn-shenzhen.aliyuncs.com
OSS_ACCESS_KEY_ID=your_access_key_id
OSS_ACCESS_KEY_SECRET=your_access_key_secret
OSS_BUCKET_NAME=your_bucket_name
OSS_URL_PREFIX=https://your-bucket-name.oss-cn-shenzhen.aliyuncs.com/

# 服务器配置
SERVER_PORT=7002
SSL_ENABLED=false
SSL_KEY_STORE=
SSL_KEY_STORE_PASSWORD=
SSL_KEY_STORE_TYPE=PKCS12

# 日志配置
LOG_FILE=/var/log/myproject/application.log

# 安全配置
REQUIRE_SSL=false

# Jenkins配置
JENKINS_URL=http://localhost:8080
JENKINS_USER=admin
JENKINS_TOKEN=your_jenkins_token

# Docker Registry配置
DOCKER_REGISTRY_URL=registry.example.com
DOCKER_REGISTRY_USER=your_registry_user
DOCKER_REGISTRY_PASSWORD=your_registry_password

# 通知配置
SLACK_TOKEN=your_slack_token
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USERNAME=your_email@example.com
EMAIL_PASSWORD=your_email_password

# 测试环境配置
TEST_OSS_ENDPOINT=http://localhost:9000
TEST_ACCESS_KEY=testkey
TEST_ACCESS_SECRET=testsecret
TEST_BUCKET=test-bucket
TEST_URL_PREFIX=http://localhost:9000/test-bucket/

# SonarQube配置
SONAR_HOST_URL=http://localhost:9000
SONAR_LOGIN=your_sonar_token

# 监控配置
PROMETHEUS_ENABLED=true
GRAFANA_URL=http://localhost:3000
